{
  "name": "causal/theodia",
  "type": "typo3-cms-extension",
  "description": "Fetch data from theodia.org to display the Mass schedule for the various places of worship in a parish or pastoral unit.",
  "keywords": [
    "TYPO3 CMS",
    "theodia",
    "church",
    "events",
    "mass"
  ],
  "homepage": "https://extensions.typo3.org/extension/theodia",
  "support": {
    "docs": "https://docs.typo3.org/p/causal/theodia/main/en-us/",
    "issues": "https://github.com/xperseguers/theodia/issues",
    "source": "https://github.com/xperseguers/theodia"
  },
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://www.causal.ch",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-3.0-or-later"
  ],
  "require": {
    "php": ">=7.4.0 <=7.4.99 || >=8.0.0 <=8.4.99",
    "ext-mbstring": "*",
    "typo3/cms-core": "^11 || ^12 || ^13"
  },
  "autoload": {
    "psr-4": {
      "Causal\\Theodia\\": "Classes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.3.x-dev"
    },
    "typo3/cms": {
      "extension-key": "theodia"
    }
  }
}
